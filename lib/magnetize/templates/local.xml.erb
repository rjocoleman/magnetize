<?xml version="1.0"?>
<config>
  <global>
    <install>
      <date><![CDATA[Thu, 01 Jul 2010 10:29:20 +0000]]></date>
    </install>

    <crypt>
      <key><![CDATA[<%= encryption_key %>]]></key>
    </crypt>

    <disable_local_modules>false</disable_local_modules>

    <resources>
      <db>
        <table_prefix><![CDATA[<%= database_table_prefix %>]]></table_prefix>
      </db>

      <default_read>
        <connection>
          <use />
          <host><![CDATA[<%= database_read_hostname %>]]></host>
          <username><![CDATA[<%= database_read_username %>]]></username>
          <password><![CDATA[<%= database_read_password %>]]></password>
          <dbname><![CDATA[<%= database_read_name %>]]></dbname>
          <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
          <model><![CDATA[mysql4]]></model>
          <type><![CDATA[pdo_mysql]]></type>
          <pdoType><![CDATA[]]></pdoType>
          <active>1</active>
        </connection>
      </default_read>

      <default_write>
        <connection>
          <use />
          <host><![CDATA[<%= database_write_hostname %>]]></host>
          <username><![CDATA[<%= database_write_username %>]]></username>
          <password><![CDATA[<%= database_write_password %>]]></password>
          <dbname><![CDATA[<%= database_write_name %>]]></dbname>
          <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
          <model><![CDATA[mysql4]]></model>
          <type><![CDATA[pdo_mysql]]></type>
          <pdoType><![CDATA[]]></pdoType>
          <active>1</active>
        </connection>
      </default_write>
    </resources>

    <session_save><![CDATA[<%= session_save %>]]></session_save>
    <session_save_path><![CDATA[<%= session_save_path %>]]></session_save_path>
    <session_cache_limiter><![CDATA[<%= session_cache_limiter %>]]></session_cache_limiter>

    <cache>
      <backend><![CDATA[<%= cache_backend %>]]></backend>
      <slow_backend><![CDATA[<%= cache_slow_backend %>]]></slow_backend>
      <slow_backend_store_data><![CDATA[<%= cache_slow_backend_store_data %>]]></slow_backend_store_data>
      <auto_refresh_fast_cache><![CDATA[<%= cache_auto_refresh_fast_cache %>]]></auto_refresh_fast_cache>

      <memcached>
        <servers>
          <server>
            <host><![CDATA[<%= cache_memcached_servers_server_host %>]]></host>
            <port><![CDATA[<%= cache_memcached_servers_server_port %>]]></port>
            <persistent><![CDATA[<%= cache_memcached_servers_server_persistent %>]]></persistent>
            <weight><![CDATA[<%= cache_memcached_servers_server_weight %>]]></weight>
            <timeout><![CDATA[<%= cache_memcached_servers_server_timeout %>]]></timeout>
            <retry_interval><![CDATA[<%= cache_memcached_servers_server_retry_interval %>]]></retry_interval>
            <status><![CDATA[<%= cache_memcached_servers_server_status %>]]></status>
          </server>
        </servers>

        <compression><![CDATA[<%= cache_memcached_compression %>]]></compression>
        <cache_dir><![CDATA[<%= cache_memcached_cache_dir %>]]></cache_dir>
        <hashed_directory_level><![CDATA[<%= cache_memcached_hashed_directory_level %>]]></hashed_directory_level>
        <hashed_directory_umask><![CDATA[<%= cache_memcached_hashed_directory_umask %>]]></hashed_directory_umask>
        <file_name_prefix><![CDATA[<%= cache_memcached_file_name_prefix %>]]></file_name_prefix>
      </memcached>
    </cache>

    <remote_addr_headers>
      <header1><![CDATA[<%= remote_addr_headers_header1 %>]]></header1>
      <header2><![CDATA[<%= remote_addr_headers_header2 %>]]></header2>
    </remote_addr_headers>
  </global>

  <admin>
    <routers>
      <adminhtml>
        <args>
          <frontName><![CDATA[<%= administrator_routers_adminhtml_args_frontname %>]]></frontName>
        </args>
      </adminhtml>
    </routers>
  </admin>
</config>
