<?xml version="1.0"?>
<config>
  <global>
    <install>
      <date><![CDATA[Thu, 01 Jul 2010 10:29:20 +0000]]></date>
    </install>

    <crypt>
      <key><![CDATA[<%= crypt_key %>]]></key>
    </crypt>

    <disable_local_modules>false</disable_local_modules>

    <resources>
      <db>
        <table_prefix><![CDATA[<%= db_table_prefix %>]]></table_prefix>
      </db>

      <default_read>
        <connection>
          <use />
          <host><![CDATA[<%= default_read_hostname %>]]></host>
          <username><![CDATA[<%= default_read_username %>]]></username>
          <password><![CDATA[<%= default_read_password %>]]></password>
          <dbname><![CDATA[<%= default_read_dbname %>]]></dbname>
          <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
          <model><![CDATA[mysql4]]></model>
          <type><![CDATA[pdo_mysql]]></type>
          <pdoType><![CDATA[]]></pdoType>
          <active>1</active>
        </connection>
      </default_read>

      <default_write>
        <connection>
          <use />
          <host><![CDATA[<%= default_write_hostname %>]]></host>
          <username><![CDATA[<%= default_write_username %>]]></username>
          <password><![CDATA[<%= default_write_password %>]]></password>
          <dbname><![CDATA[<%= default_write_dbname %>]]></dbname>
          <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
          <model><![CDATA[mysql4]]></model>
          <type><![CDATA[pdo_mysql]]></type>
          <pdoType><![CDATA[]]></pdoType>
          <active>1</active>
        </connection>
      </default_write>

      <default_setup>
        <connection>
          <use />
          <host><![CDATA[<%= default_setup_hostname || default_write_hostname %>]]></host>
          <username><![CDATA[<%= default_setup_username || default_write_username %>]]></username>
          <password><![CDATA[<%= default_setup_password || default_write_password %>]]></password>
          <dbname><![CDATA[<%= default_setup_dbname || default_write_dbname %>]]></dbname>
          <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
          <model><![CDATA[mysql4]]></model>
          <type><![CDATA[pdo_mysql]]></type>
          <pdoType><![CDATA[]]></pdoType>
          <active>1</active>
        </connection>
      </default_setup>
    </resources>

    <% if session_save %><session_save><![CDATA[<%= session_save %>]]></session_save><% end %>
    <% if session_save_path %><session_save_path><![CDATA[<%= session_save_path %>]]></session_save_path><% end %>
    <% if session_cache_limiter %><session_cache_limiter><![CDATA[<%= session_cache_limiter %>]]></session_cache_limiter><% end %>

    <% if cache_slow_backend || cache_fast_backend || cache_backend %>
    <cache>
      <% if cache_slow_backend %>
      <slow_backend><![CDATA[<%= cache_slow_backend %>]]></slow_backend>
      <slow_backend_options>
        <servers>
          <server>
            <host><![CDATA[<%= cache_slow_backend_servers_server_host %>]]></host>
            <port><![CDATA[<%= cache_slow_backend_servers_server_port %>]]></port>
            <% if cache_slow_backend_servers_server_persistent %><persistent><![CDATA[<%= cache_slow_backend_servers_server_persistent %>]]></persistent><% end %>
          </server>
        </servers>
      </slow_backend_options>
      <% end %>
      
      <% if cache_fast_backend %>
      <fast_backend><![CDATA[<%= cache_fast_backend %>]]></fast_backend>
      <fast_backend_options>
        <servers>
          <server>
            <host><![CDATA[<%= cache_fast_backend_servers_server_host %>]]></host>
            <port><![CDATA[<%= cache_fast_backend_servers_server_port %>]]></port>
            <% if cache_fast_backend_servers_server_persistent %><persistent><![CDATA[<%= cache_fast_backend_servers_server_persistent %>]]></persistent><% end %>
          </server>
        </servers>
      </fast_backend_options>
      <% end %>

      <% if cache_backend %>
      <backend><![CDATA[<%= cache_backend %>]]></backend>
      <% end %>
      <% if cache_backend && cache_memcached_servers_server_host %>
      <memcached>
        <servers>
          <server>
            <host><![CDATA[<%= cache_memcached_servers_server_host %>]]></host>
            <port><![CDATA[<%= cache_memcached_servers_server_port %>]]></port>
            <% if cache_memcached_servers_server_persistent %><persistent><![CDATA[<%= cache_memcached_servers_server_persistent %>]]></persistent><% end %>
          </server>
        </servers>
        <% if cache_memcached_compression %><compression><![CDATA[<%= cache_memcached_compression %>]]></compression><% end %>
        <% if cache_memcached_cache_dir %><cache_dir><![CDATA[<%= cache_memcached_cache_dir %>]]></cache_dir><% end %>
        <% if cache_memcached_hashed_directory_level %><hashed_directory_level><![CDATA[<%= cache_memcached_hashed_directory_level %>]]></hashed_directory_level><% end %>
        <% if cache_memcached_hashed_directory_umask %><hashed_directory_umask><![CDATA[<%= cache_memcached_hashed_directory_umask %>]]></hashed_directory_umask><% end %>
        <% if cache_memcached_file_name_prefix %><file_name_prefix><![CDATA[<%= cache_memcached_file_name_prefix %>]]></file_name_prefix><% end %>
      </memcached>
      <% end %>
    </cache>
    <% end %>

    <% if remote_addr_headers_header1 || remote_addr_headers_header2 %>
    <remote_addr_headers>
      <header1><![CDATA[<%= remote_addr_headers_header1 %>]]></header1>
      <header2><![CDATA[<%= remote_addr_headers_header2 %>]]></header2>
    </remote_addr_headers>
    <% end %>
  </global>

  <% if admin_routers_adminhtml_args_frontname %>
  <admin>
    <routers>
      <adminhtml>
        <args>
          <frontName><![CDATA[<%= admin_routers_adminhtml_args_frontname %>]]></frontName>
        </args>
      </adminhtml>
    </routers>
  </admin>
  <% end %>
</config>
